<% include ./partials/header %>
    <div class="container">
        <div class="row">
            <div class="form-title">
                <h1>Please Insert Training Set</h1>
            </div>
            <div class="form-body">
                <form action="/train/upload" enctype="multipart/form-data" method="post">
                    <div class="form-group">
                        <label>Name</label>
                        <input class="form-control" type="text" name="name" placeholder="Name">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="file" name="photos" multiple>
                    </div>
                    <input class="btn btn-primary" type="submit" value="Upload photo">
                </form>
            </div>
        </div>
    </div>
<% include ./partials/footer %>